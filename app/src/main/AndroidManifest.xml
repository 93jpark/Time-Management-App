        </activity>
        <activity android:name=".Home"/>
        <activity android:name=".Signup"></activity>
        <activity android:name=".Add"></activity>
        <activity android:name=".BarChart"></activity>
        <activity android:name=".PieChart"></activity>
        <activity android:name=".StopWatch"></activity>

    </application>

</manifest>